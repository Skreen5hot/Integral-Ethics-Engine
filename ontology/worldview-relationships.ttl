# ============================================================================
# INTEGRATION STRATEGIES (BFO PROCESSES)
# ============================================================================

:DialecticalSynthesis a :IntegrationStrategy ;
    dcterms:source "Hegel (1807): Thesis-Antithesis-Synthesis dialectical progression" ;
    rdfs:label "Dialectical Synthesis Process" ;
    skos:definition "An integration process that preserves insights of both opposing worldviews in higher-order synthesis. Modeled as BFO process with inputs (conflicting worldviews) and output (synthesized understanding)." ;
    bfo:0000057 :Agent ;  # has_participant (agent performs synthesis)
    cco:has_input :ComplementaryPair ;
    cco:has_output :SynthesizedWorldviewUnderstanding ;
    :processStructure [
        :step1 "Identify thesis and antithesis worldviews" ;
        :step2 "Recognize partial truth in each via critical analysis" ;
        :step3 "Construct synthesis transcending both while incorporating their truths"
    ] ;
    :applicableTo :ComplementaryPair ;
    :examples [
        :example1 "Kant's synthesis of rationalism and empiricism (Critique of Pure Reason)" ;
        :example2 "Hegel's synthesis of realism and idealism in absolute idealism" ;
        :example3 "Whitehead's synthesis of being and becoming in process philosophy"
    ] ;
    :limitation "Requires acceptance of higher dialectical level; not all conflicts admit synthesis" ;
    :empiricalValidation "Limited empirical research on dialectical synthesis as cognitive process; remains primarily philosophical method" ;
    rdfs:comment "Classic Hegelian approach; powerful but not universally applicable. BFO-compliant modeling as process with defined inputs/outputs." .

:DomainContextualization a :IntegrationStrategy ;
    dcterms:source "Gould (1997): NOMA - non-overlapping magisteria for science and religion. Hedlund-de Witt (2012): Domain-specific worldview appropriateness." ;
    rdfs:label "Domain Contextualization Process" ;
    skos:definition "An integration process separating worldviews by domain of application (e.g., material vs. spiritual, science vs. art). Modeled as BFO process." ;
    bfo:0000057 :Agent ;
    cco:has_input :WorldviewConflict ;
    cco:has_output :DomainSeparatedWorldviews ;
    :processStructure [
        :step1 "Identify proper domain of each worldview" ;
        :step2 "Apply each worldview within its appropriate domain" ;
        :step3 "Recognize limitations when worldview applied outside proper domain"
    ] ;
    :applicableTo :MaterialismSpiritualismConflict ;
    :examples [
        :example1 "Materialism for scientific explanation; Spiritualism for religious meaning (Gould's NOMA)" ;
        :example2 "Rationalism for mathematics; Sensationalism for aesthetic appreciation" ;
        :example3 "Realism for physics; Idealism for ethics"
    ] ;
    :limitation "Doesn't resolve conflicts within overlapping domains; boundary disputes common" ;
    :empiricalSupport [
        :study1 "Hedlund-de Witt (2012): Sustainable lifestyles require domain-appropriate worldviews (integrating material, social, spiritual)" ;
        :study2 "Gould (1997): NOMA prevents conflict by domain separation, though critics argue domains do overlap"
    ] ;
    rdfs:comment "Pragmatic solution separating incompatible worldviews by context. Empirically supported for sustainability applications." .

:PerspectivalPluralism a :IntegrationStrategy ;
    dcterms:source "Wilber (2000): All perspectives are true but partial. Choi et al. (2007): Individual differences in perspectival flexibility." ;
    rdfs:label "Perspectival Pluralism Process" ;
    skos:definition "An integration process recognizing multiple valid perspectives on reality without privileging one as absolute truth. Modeled as BFO process requiring perspectival flexibility." ;
    bfo:0000057 :Agent ;
    cco:has_input :IrreconcilableMetaphysicalConflict ;
    cco:has_output :MultiPerspectivalUnderstanding ;
    :processStructure [
        :step1 "Acknowledge validity of each worldview within its perspective" ;
        :step2 "Recognize partiality of all perspectives (epistemic humility)" ;
        :step3 "Cultivate capacity to inhabit multiple perspectives (cognitive flexibility)"
    ] ;
    :applicableTo :IrreconcilableMetaphysicalConflicts ;
    :requiredCapacity :PerspectivalFlexibility ;
    :examples [
        :example1 "Wilber's integral theory: all quadrants are valid perspectives on holons" ;
        :example2 "Ferrer's participatory pluralism: multiple spiritual paths are valid" ;
        :example3 "Esbjörn-Hargens' integral ecology: ecological crises require multiple perspectives"
    ] ;
    :empiricalEvidence [
        :study1 "Van Egmond & De Vries (2011): Integral worldview (multi-perspectival) predicts sustainability commitment" ;
        :study2 "Choi et al. (2007): Individuals vary in ability to adopt holistic vs. analytic perspectives; flexibility trainable" ;
        :study3 "Nisbett (2003): Cross-cultural exposure increases perspectival flexibility"
    ] ;
    :limitation "Risks relativism if no criteria for perspective evaluation; requires meta-framework" ;
    rdfs:comment "Postmodern strategy; avoids privileging any single worldview while maintaining rigor. Empirical support for cognitive flexibility." .

:HierarchicalIntegration a :IntegrationStrategy ;
    dcterms:source "Wilber (1995): Transcend and include - higher levels integrate lower levels. Gebser (1949): Consciousness structure evolution." ;
    rdfs:label "Hierarchical Integration Process" ;
    skos:definition "An integration process organizing worldviews into developmental hierarchy where later stages transcend but include earlier stages. Modeled as BFO process with temporal ordering." ;
    bfo:0000057 :Agent ;
    cco:has_input :DevelopmentalSequence ;
    cco:has_output :IntegratedHierarchicalUnderstanding ;
    :processStructure [
        :step1 "Identify developmental sequence of worldviews" ;
        :step2 "Show how later stages incorporate truths of earlier stages while adding novel capacities" ;
        :step3 "Recognize appropriate contexts for each developmental level"
    ] ;
    :wilberianStructure "Atoms → Molecules → Cells → Organisms → Minds → Souls → Spirit (each level transcends and includes previous)" ;
    :applicableTo :DevelopmentalSequences ;
    :limitation "Assumes developmental hierarchy; criticized as hierarchical bias and ethnocentrism" ;
    :empiricalSupport [
        :study1 "Gebser (1949): Consciousness structures evolve through archaic, magical, mythical, mental, integral stages (historical/cultural analysis)" ;
        :study2 "Cook-Greuter (2004): Ego development stages show hierarchical integration (empirical n=4,510)" ;
        :critique "Critics argue hierarchy reflects Western bias; non-hierarchical integration models (e.g., heterarchy) also valid"
    ] ;
    :controversy "Heavily debated; supporters cite developmental psychology; critics cite cultural bias" ;
    rdfs:comment "Integral theory's signature strategy; controversial but influential. Empirical support from developmental psychology but critiqued for potential ethnocentrism." .

# ============================================================================
# INTEGRATION PROCESS OUTPUTS (BFO INFORMATION CONTENT ENTITIES)
# ============================================================================

:IntegratedWorldviewUnderstanding a owl:Class ;
    rdfs:subClassOf bfo:0000031 ;  # generically dependent continuant (information content entity)
    rdfs:label "Integrated Worldview Understanding" ;
    skos:definition "An information content entity representing a synthesized understanding that incorporates insights from multiple worldviews while recognizing their distinct perspectives." ;
    :generatedBy :IntegrationStrategy ;
    rdfs:comment "Output of successful integration process. ICE (information content entity) in BFO." .

:SynthesizedWorldviewUnderstanding a owl:Class ;
    rdfs:subClassOf :IntegratedWorldviewUnderstanding ;
    rdfs:label "Synthesized Worldview Understanding" ;
    skos:definition "ICE representing dialectical synthesis (thesis + antithesis → synthesis)." ;
    :generatedBy :DialecticalSynthesis ;
    rdfs:comment "Specific type of integrated understanding via Hegelian synthesis." .

:DomainSeparatedWorldviews a owl:Class ;
    rdfs:subClassOf :IntegratedWorldviewUnderstanding ;
    rdfs:label "Domain-Separated Worldviews" ;
    skos:definition "ICE representing recognition of appropriate domains for different worldviews." ;
    :generatedBy :DomainContextualization ;
    rdfs:comment "Integration via domain separation (e.g., NOMA)." .

:MultiPerspectivalUnderstanding a owl:Class ;
    rdfs:subClassOf :IntegratedWorldviewUnderstanding ;
    rdfs:label "Multi-Perspectival Understanding" ;
    skos:definition "ICE representing capacity to hold multiple worldview perspectives simultaneously without privileging one as absolute." ;
    :generatedBy :PerspectivalPluralism ;
    :requiresCapacity :PerspectivalFlexibility ;
    rdfs:comment "Integration via perspectival pluralism; requires cognitive flexibility." .

# ============================================================================
# EMPIRICAL WORLDVIEW MEASUREMENT
# ============================================================================

:WorldviewAssessmentProcess a owl:Class ;
    rdfs:subClassOf bfo:0000015 ;  # process
    dcterms:source "Hedlund-de Witt et al. (2014): Integrative Worldview Framework (IWF) for empirical measurement" ;
    rdfs:label "Worldview Assessment Process" ;
    skos:definition "A measurement process determining an individual's or group's worldview orientation using validated instruments." ;
    bfo:0000057 :Agent ;  # has_participant (person being assessed)
    cco:has_output :WorldviewProfile ;
    :validatedInstruments [
        :IWF "Integrative Worldview Framework (Hedlund-de Witt 2014): 4 worldview types × 5 dimensions = 20-item scale" ;
        :WVS "World Values Survey: Materialist vs. Post-materialist values" ;
        :note "No validated instrument yet exists for all 12 IEE worldviews; proposed future development"
    ] ;
    :empiricalEvidence "Hedlund-de Witt et al. (2014): IWF shows good reliability (Cronbach's α > 0.70) and construct validity (n=478)" ;
    rdfs:comment "Empirical measurement enables quantitative worldview research. IEE's 12-worldview system needs instrument development." .

:WorldviewProfile a owl:Class ;
    rdfs:subClassOf bfo:0000031 ;  # ICE
    rdfs:label "Worldview Profile" ;
    skos:definition "An information content entity representing measured worldview orientations of an individual or group across multiple worldview dimensions." ;
    :generatedBy :WorldviewAssessmentProcess ;
    :typicallyIncludes "Scores or rankings on Material-Spiritual axis, Object-Subject axis, Static-Dynamic axis" ;
    rdfs:comment "Empirical profile from validated assessment. Enables research on worldview correlates (values, behaviors, attitudes)." .

