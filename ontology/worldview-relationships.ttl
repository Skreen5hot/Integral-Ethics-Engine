# ============================================================================
# WORLDVIEW REFERENCE (Defined Elsewhere)
# ============================================================================

# The 12 worldviews are defined in their respective ontology files.
# This file models only the relationships BETWEEN worldviews.

:Worldview a owl:Class ;
    rdfs:label "Worldview" ;
    skos:definition "A comprehensive framework of beliefs, values, and assumptions about the fundamental nature of reality, knowledge, and value." ;
    dcterms:source "Koltko-Rivera (2004): Worldview as set of presuppositions about basic constituents of reality" ;
    rdfs:comment "Each of the 12 worldviews is defined in its own ontology file (e.g., materialism-values.ttl, sensationalism-values.ttl)." ;
    rdfs:seeAlso :Materialism, :Realism, :Sensationalism, :Phenomenalism,
                 :Dynamism, :Monadism, :Idealism, :Rationalism,
                 :Psychism, :Pneumatism, :Spiritualism, :Mathematism .

# ============================================================================
# CORE RELATIONSHIP TYPES (BFO-COMPLIANT)
# ============================================================================

:WorldviewRelationship a owl:Class ;
    rdfs:subClassOf bfo:0000015 ;  # process
    dcterms:source "Koltko-Rivera (2004): Worldviews form systems with structural relationships. BFO modeling: relationships as ongoing processes." ;
    rdfs:label "Worldview Relationship Process" ;
    skos:definition "A process of relating two or more worldviews through comparison, integration, or conflict resolution. Modeled as BFO process." ;
    bfo:0000057 :Worldview ;  # has_participant (worldviews participate in relationship process)
    rdfs:comment "Relationships between worldviews are not static facts but ongoing processes of comparison, dialogue, and potential integration. BFO-compliant modeling as process." .

:ComplementaryPair a owl:Class ;
    rdfs:subClassOf :WorldviewRelationship ;
    dcterms:source "Gebser (1949), Wilber (2000): Integral structure includes complementary opposites. Empirical support: Nisbett (2001) documents analytic-holistic cognitive complementarity." ;
    rdfs:label "Complementary Pair Relationship" ;
    skos:definition "A relationship process recognizing two worldviews as opposite extremes along a single dimension, requiring integration for completeness. Modeled as BFO process with integration as potential outcome." ;
    :integrationOutputClass :IntegratedWorldviewUnderstanding ;
    :empiricalEvidence "Nisbett et al. (2001): Analytic (object-focused) and holistic (context-focused) cognition are complementary systems across cultures" ;
    rdfs:comment "Complementary pairs are not contradictories (which cannot both be true) but contraries that capture different aspects of reality requiring synthesis." .

:WorldviewConflict a owl:Class ;
    rdfs:subClassOf :WorldviewRelationship ;
    dcterms:source "Nilsson et al. (2020): Worldview conflict as source of prejudice and misunderstanding" ;
    rdfs:label "Worldview Conflict Process" ;
    skos:definition "A process of tension or incompatibility between worldviews regarding fundamental ontological, epistemic, or axiological claims. Modeled as BFO process with resolution as potential outcome." ;
    :resolutionOutputClass :ResolvedWorldviewConflict ;
    :empiricalEvidence "Nilsson et al. (2020): Worldview conflicts predict intergroup prejudice and reduced cooperation" ;
    rdfs:comment "Conflicts may be resolvable (via domain separation) or irresolvable (requiring perspectival pluralism). Resolution is a process outcome." .

:WorldviewCluster a owl:Class ;
    rdfs:subClassOf bfo:0000027 ;  # object aggregate
    dcterms:source "Wilber (2000): Worldviews cluster into quadrants and levels" ;
    rdfs:label "Worldview Cluster" ;
    skos:definition "An object aggregate (BFO) of worldviews sharing fundamental ontological commitments or epistemic stances." ;
    bfo:0000051 :Worldview ;  # has_part (cluster has worldview as parts)
    :empiricalEvidence "Hedlund-de Witt et al. (2014): Quantitative worldview survey reveals clustering patterns matching theoretical predictions" ;
    rdfs:comment "Clusters represent 'families' of related worldviews with natural affinities. BFO-compliant modeling as object aggregate." .

:IntegrationStrategy a owl:Class ;
    rdfs:subClassOf bfo:0000015 ;  # process
    dcterms:source "Ricoeur (1970): Hermeneutics of conflict requires interpretive integration" ;
    rdfs:label "Integration Strategy Process" ;
    skos:definition "A process for synthesizing or coordinating multiple worldviews while preserving their distinctive insights. Modeled as BFO process." ;
    :hasInputClass :WorldviewConflict ;
    :hasOutputClass :IntegratedWorldviewUnderstanding ;
    :subtypes :DialecticalSynthesis, :DomainContextualization, :PerspectivalPluralism, :HierarchicalIntegration ;
    rdfs:comment "Different conflicts require different integration strategies - no one-size-fits-all solution. Each strategy is a distinct process type." .

# ============================================================================
# BFO-COMPLIANT RELATIONSHIP DISPOSITIONS
# ============================================================================

:ComplementarityDisposition a owl:Class ;
    rdfs:subClassOf bfo:0000016 ;  # disposition
    dcterms:source "BFO pattern: Worldview pairs have disposition toward complementary integration" ;
    rdfs:label "Complementarity Disposition" ;
    skos:definition "A disposition of a worldview pair to undergo complementary integration when conditions are appropriate (e.g., when holder recognizes partiality of single perspective)." ;
    bfo:0000052 :WorldviewPair ;  # inheres_in
    bfo:0000054 :ComplementaryPair ;  # realized_in (this disposition realized in complementary pair process)
    rdfs:comment "Complementarity is not mere fact but a disposition that may or may not be realized depending on epistemic humility and integral awareness." .

:ConflictDisposition a owl:Class ;
    rdfs:subClassOf bfo:0000016 ;
    dcterms:source "BFO pattern: Incompatible worldview pairs have disposition toward conflict" ;
    rdfs:label "Conflict Disposition" ;
    skos:definition "A disposition of incompatible worldview pairs to generate conflict when brought into contact (e.g., when holders dialogue)." ;
    bfo:0000052 :WorldviewPair ;
    bfo:0000054 :WorldviewConflict ;  # realized_in
    :empiricalEvidence "Nilsson et al. (2020): Worldview distance predicts likelihood and intensity of intergroup conflict" ;
    rdfs:comment "Conflict disposition realized when worldview holders with incompatible commitments interact without integration strategy." .

# ============================================================================
# WORLDVIEW CLUSTERS (PHASE 1: MATERIAL-EMPIRICAL)
# ============================================================================

:MaterialEmpiricalCluster a :WorldviewCluster ;
    dcterms:source "Wilber (2000): Lower-right quadrant emphasis (material/empirical). Hedlund-de Witt (2014): Modern worldview cluster." ;
    rdfs:label "Material-Empirical Cluster" ;
    skos:definition "An object aggregate of worldviews grounding knowledge and value in material substance and empirical observation." ;
    bfo:0000051 :Materialism, :Realism, :Sensationalism, :Phenomenalism ;  # has_part
    :sharedCommitments :EmpiricalVerification, :MaterialSubstance, :SensoryEpistemology ;
    :emphasizes "Matter, physical reality, sensory experience, empirical data" ;
    :subordinates "Spirit, consciousness, abstract forms, subjective meaning" ;
    :empiricalEvidence [
        :study1 "Van Egmond & De Vries (2011): Material-empirical worldviews dominate modern Western science and economics" ;
        :study2 "Hedlund-de Witt et al. (2014): Modern worldview cluster (materialist-reductionist) identified in quantitative survey (n=478)" ;
        :prevalence "Dominant in Western academia, scientific institutions, and secular culture"
    ] ;
    rdfs:comment "Phase 1 worldviews share materialist-empiricist foundations despite internal differences. BFO modeling as object aggregate of 4 worldviews." .

:MaterialismRealism_Alignment a :WorldviewRelationship ;
    dcterms:source "Both worldviews underlie scientific realism in philosophy of science" ;
    :between :Materialism, :Realism ;
    :relationshipType :StrongAlignment ;
    :sharedCommitments :MindIndependentReality, :CausalDeterminism, :ScientificMethodology ;
    :minorDifferences "Materialism: Matter is fundamental. Realism: Objects (which may be material or formal) are fundamental." ;
    :empiricalSupport "Nisbett (2003): Analytic cognition (favored by both) dominant in Western scientific culture" ;
    rdfs:comment "Strongest alignment within Material-Empirical cluster. Both support scientific realism." .

:SensationalismPhenomenalism_Affinity a :WorldviewRelationship ;
    dcterms:source "Both influenced by British Empiricism (Berkeley, Hume, Mill)" ;
    :between :Sensationalism, :Phenomenalism ;
    :relationshipType :Affinity ;
    :sharedCommitments :PhenomenalPrimacy, :ExperientialEpistemology, :SkepticismAboutSubstance ;
    :keyDifference "Sensationalism: Quality of sensations (aesthetic). Phenomenalism: Validity of appearances (epistemic)." ;
    rdfs:comment "Share phenomenal starting point but diverge on axiological vs. epistemic focus." .

# ============================================================================
# WORLDVIEW CLUSTERS (PHASE 2: PROCESS-INDIVIDUAL)
# ============================================================================

:ProcessIndividualCluster a :WorldviewCluster ;
    dcterms:source "Whitehead (1929), Wilber (2000): Process philosophy and individual uniqueness. Nisbett (2001): Process ontology vs. entity ontology across cultures." ;
    rdfs:label "Process-Individual Cluster" ;
    skos:definition "An object aggregate of worldviews emphasizing dynamic becoming, temporal processes, and individual uniqueness." ;
    bfo:0000051 :Dynamism, :Monadism, :Idealism, :Rationalism ;
    :sharedCommitments :ProcessOntology, :IndividualUniqueness, :TemporalPrimacy ;
    :emphasizes "Becoming, development, individual perspective, creative emergence" ;
    :subordinates "Static being, generic types, timeless truths" ;
    :empiricalEvidence [
        :study1 "Rescher (1996): Process metaphysics challenges substance ontology dominance" ;
        :study2 "Nisbett et al. (2001): East Asian cultures favor process/relationship ontologies over entity ontologies" ;
        :cross_cultural_note "Process-individual emphasis varies by culture; more prominent in East Asian and Indigenous worldviews"
    ] ;
    rdfs:comment "Phase 2 worldviews introduce temporality and individual uniqueness as fundamental. Less dominant than Phase 1 in modern West but prevalent in other cultures." .

:DynamismIdealism_Tension a :WorldviewRelationship ;
    dcterms:source "Whitehead (1929): Process and Reality synthesizes process and idealism" ;
    :between :Dynamism, :Idealism ;
    :relationshipType :CreativeTension ;
    :dynamismEmphasis "Material-energetic flux, vital force, embodied becoming" ;
    :idealismEmphasis "Consciousness clarity, mental structures, rational order" ;
    :integrationPossibility "Process idealism (Whitehead): Consciousness unfolds through creative becoming" ;
    :empiricalNote "Nisbett (2001): Tension mirrors Western (idealist-rational) vs. Eastern (dynamic-holistic) cognitive styles" ;
    rdfs:comment "Tension between energetic flux and rational clarity; synthesis possible via process idealism." .

# ============================================================================
# WORLDVIEW CLUSTERS (PHASE 3: DEPTH-SPIRITUAL)
# ============================================================================

:DepthSpiritualCluster a :WorldviewCluster ;
    dcterms:source "Wilber (2000): Upper-left quadrant emphasis (interior depth/spirituality). Vail et al. (2012): Religious worldviews as terror management." ;
    rdfs:label "Depth-Spiritual Cluster" ;
    skos:definition "An object aggregate of worldviews grounding reality in spiritual depth, transcendent dimensions, or numinous presence." ;
    bfo:0000051 :Psychism, :Pneumatism, :Spiritualism, :Mathematism ;
    :sharedCommitments :TranscendentReality, :InteriorDepth, :SpiritualPrimacy ;
    :emphasizes "Spirit, depth, transcendence, numinous, sacred, archetypal forms" ;
    :subordinates "Material surface, sensory immediacy, empirical verification" ;
    :empiricalEvidence [
        :study1 "Ferrer (2017): Participatory spirituality recognizes diverse paths to transcendence" ;
        :study2 "Vail et al. (2012): Spiritual worldviews serve terror management function (meaning in face of mortality)" ;
        :study3 "Hedlund-de Witt et al. (2014): Spiritual worldview cluster identified in survey (distinct from modern and postmodern)" ;
        :prevalence "Dominant in religious traditions, depth psychology, mysticism; minority in Western academia"
    ] ;
    rdfs:comment "Phase 3 worldviews prioritize spiritual/depth dimensions over material surface. Cultural prevalence varies dramatically." .

:SpiritualismPneumatism_Affinity a :WorldviewRelationship ;
    dcterms:source "Both central to religious worldviews across traditions" ;
    :between :Spiritualism, :Pneumatism ;
    :relationshipType :StrongAffinity ;
    :sharedCommitments :SpiritualReality, :SacredCosmos, :DivinePrimacy ;
    :keyDifference "Spiritualism: Transcendent spirit beyond matter. Pneumatism: Immanent spirit pervading matter." ;
    :integrationStrategy "Panentheism: God both transcends and pervades creation" ;
    :empiricalEvidence "Cross-cultural religious studies: transcendent vs. immanent conceptions of divine vary by tradition but often coexist" ;
    rdfs:comment "Closest affinity within Depth-Spiritual cluster; differ on transcendence vs. immanence." .

# ============================================================================
# COMPLEMENTARY PAIRS - AXIS 1: MATERIAL ↔ SPIRITUAL
# ============================================================================

:MaterialismSpiritualismPair a :ComplementaryPair ;
    dcterms:source "Wilber (1995): Fundamental dialectic between matter and spirit. Empirical: Vail et al. (2012) document materialism vs. spirituality as worldview dimension." ;
    rdfs:label "Materialism ↔ Spiritualism" ;
    bfo:0000057 :Materialism, :Spiritualism ;  # has_participant
    :axis :MaterialSpiritualAxis ;
    :tension "Matter vs. Spirit Primacy" ;
    :materialismClaim "Physical matter is fundamental; consciousness is emergent from material complexity" ;
    :spiritualismClaim "Transcendent spirit is fundamental; matter is emanation or manifestation of spirit" ;
    :irreconcilableMetaphysics true ;
    :integrationStrategy :DomainContextualization ;
    :integrationExample "Science studies material domain (Materialism); Religion studies spiritual domain (Spiritualism)" ;
    :dialecticalSynthesis "Panentheism or dual-aspect monism: matter and spirit are two aspects of one reality" ;
    :empiricalEvidence [
        :study1 "Johnson et al. (2011): Material vs. spiritual worldviews predict different social attitudes (n=186)" ;
        :study2 "Vail et al. (2012): Materialist worldview associated with lower death anxiety but higher meaninglessness" ;
        :study3 "Greenberg et al. (1997): Terror management theory shows worldviews (especially spiritual) buffer mortality salience"
    ] ;
    rdfs:comment "Most fundamental complementary pair - represents deepest ontological divide. Empirically validated as key worldview dimension." .

