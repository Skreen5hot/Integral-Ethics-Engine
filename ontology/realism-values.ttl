:ScientificDiscovery a owl:Class ;
    rdfs:subClassOf bfo:0000015 ;  # process
    dcterms:source "Sellars (1963): Scientific realism - discovering mind-independent theoretical entities through abductive inference" ;
    rdfs:label "Scientific Discovery Process" ;
    skos:definition "A BFO process of discovering objective truths about mind-independent reality through systematic empirical investigation and theoretical inference." ;
    bfo:0000057 :Scientist, :NaturalPhenomenon ;  # has_participant
    :hasSubProcesses :Observation, :HypothesisFormation, :ExperimentalTesting, :TheoryRevision ;
    cco:has_input :NaturalPhenomenon, :EmpiricalData ;
    cco:has_output :ScientificKnowledge, :ObjectiveTruth ;
    :realizesValue :ObjectiveTruth, :CorrespondenceToReality ;
    :realistFeatures [
        :assumes_external_world "Science assumes mind-independent natural phenomena to investigate" ;
        :aims_at_truth "Not just empirical adequacy but truth about unobservables" ;
        :convergence "Successive theories converge on truth (Psillos)" ;
        :success_explained "Predictive success explained by approximate truth (no miracles)"
    ] ;
    :examples [
        :atoms "Atoms inferred from chemical regularities, confirmed by multiple methods" ;
        :DNA "Double helix structure discovered through X-ray crystallography, explains heredity" ;
        :quarks "Quarks as constituents of protons/neutrons, initially unobservable theoretical entities"
    ] ;
    rdfs:comment "Scientific discovery is paradigmatic realist process: Assumes objective reality, discovers truths about unobservables, generates reliable knowledge." .

:CorrespondenceVerification a owl:Class ;
    rdfs:subClassOf bfo:0000015 ;  # process
    dcterms:source "Tarski (1944): Semantic conception of truth - truth as correspondence verified through observation" ;
    rdfs:label "Correspondence Verification Process" ;
    skos:definition "A BFO process of checking whether beliefs or representations correspond to mind-independent reality through empirical testing." ;
    bfo:0000057 :Investigator, :RealityItself ;
    cco:has_input :Belief, :Proposition, :Theory ;
    cco:has_output :VerificationResult, :TruthValue ;
    :realizesValue :ObjectiveTruth, :FactualAccuracy, :EmpiricalVerification ;
    :processSteps [
        :step1 "Articulate testable claim derived from belief/theory" ;
        :step2 "Design observation or experiment to test claim" ;
        :step3 "Execute observation/experiment in contact with reality" ;
        :step4 "Compare predicted observation to actual observation" ;
        :step5 "Infer correspondence (true) or non-correspondence (false)"
    ] ;
    :assumesRealism "Process requires: (1) Mind-independent reality, (2) Empirical access to it, (3) Correspondence relation between beliefs and reality" ;
    :yieldedKnowledge "Truth values: Which propositions correspond to reality" ;
    rdfs:comment "Verification process embodies realist epistemology: Check beliefs against reality itself, not coherence or consensus alone." .

:RationalInquiry a owl:Class ;
    rdfs:subClassOf bfo:0000015 ;  # process
    dcterms:source "Aristotle: Rational investigation as path to knowledge of reality. Reid (1764): Common sense realism grounded in rational faculties." ;
    rdfs:label "Rational Inquiry Process" ;
    skos:definition "A BFO process of pursuing objective truth through logical reasoning, critical examination, and inference from evidence." ;
    bfo:0000057 :RationalAgent ;
    cco:has_input :Evidence, :Arguments, :Questions ;
    cco:has_output :ReasonedBeliefs, :Justifications, :Knowledge ;
    :realizesValue :ObjectiveTruth, :RationalConsistency, :CriticalScrutiny ;
    :methodologicalFeatures [
        :deductive_inference "Valid deduction preserves truth from premises to conclusions" ;
        :inductive_inference "Induction from observed regularities to generalizations" ;
        :abductive_inference "Inference to best explanation of observed phenomena" ;
        :critical_examination "Scrutinize arguments for validity, premises for truth"
    ] ;
    :assumes :ObjectiveLogicalTruths, :RationalFacultiesTrackReality ;
    :yields "Rationally justified beliefs about objective reality" ;
    rdfs:comment "Rational inquiry realizes realist values: Reason as faculty for discovering objective truth about mind-independent reality." .

