# ============================================================================
# BFO PROCESS MODELING - Materialist Processes
# ============================================================================

:MaterialisticReduction a owl:Class ;
    rdfs:subClassOf bfo:0000015 ;  # process
    dcterms:source "Churchland (1981): Eliminative materialism reduces mental states to neuroscientific descriptions" ;
    rdfs:label "Materialistic Reduction Process" ;
    skos:definition "A BFO process of explaining apparently non-material phenomena (consciousness, meaning, beauty) in terms of underlying material/physical mechanisms." ;
    bfo:0000057 :MaterialistPhilosopher ;  # has_participant
    :hasSubProcesses :IdentifyApparentlyNonMaterial, :SeekMaterialBasis, :ProvideReductiveExplanation ;
    cco:has_input :NonMaterialPhenomenon ;
    cco:has_output :MaterialistExplanation ;
    :reductionStrategies [
        :typeIdentity "Mental state tokens are identical to brain state tokens (Smart, Place)" ;
        :functionalism "Mental states are functional roles implementable in physical systems (Putnam, Fodor)" ;
        :eliminativism "Folk psychology concepts eliminated in favor of neuroscience (Churchland)" ;
        :supervenience "Mental supervenes on physical without reduction (Davidson, Kim)"
    ] ;
    :examples [
        :consciousness "Consciousness = neural activity patterns in global workspace (Baars, Dehaene)" ;
        :pain "Pain = C-fiber firing + associated neural cascades (Place 1956)" ;
        :belief "Belief = functional state of brain disposed to guide behavior"
    ] ;
    rdfs:comment "Reduction is characteristic materialist move: Show apparent non-material phenomena have material basis. Makes materialism empirically testable." .

:EmpiricalMeasurement a owl:Class ;
    rdfs:subClassOf bfo:0000015 ;  # process
    dcterms:source "Stevens (1946): Theory of measurement scales. Operationalism: Concepts defined by measurement operations." ;
    rdfs:label "Empirical Measurement Process" ;
    skos:definition "A BFO process of quantifying material properties through instrumentation and observation, yielding empirical data." ;
    bfo:0000057 :Scientist, :Instrument ;
    cco:has_input :MaterialProperty ;
    cco:has_output :MeasurementData ;
    :realizesValue :Measurement, :EmpiricalTruth ;
    :processSteps [
        :step1 "Select measurable material property (mass, length, temperature, neural firing rate)" ;
        :step2 "Apply standardized measurement instrument (scale, ruler, thermometer, electrode)" ;
        :step3 "Record quantitative value with units and precision" ;
        :step4 "Replicate measurement to ensure reliability"
    ] ;
    :yieldedKnowledge "Quantitative empirical facts about material world" ;
    :empiricalSupport "Measurement theory (Krantz et al. 1971): Formal theory of representation and uniqueness of measurement" ;
    rdfs:comment "Measurement is paradigmatic materialist epistemology: Gain knowledge through interaction with material world via instruments. Only measurable properties are scientifically real." .

:ExperimentalVerification a owl:Class ;
    rdfs:subClassOf bfo:0000015 ;  # process
    dcterms:source "Popper (1959): Experimental testing as attempted falsification. Mill (1843): Methods of experimental inquiry." ;
    rdfs:label "Experimental Verification Process" ;
    skos:definition "A BFO process of testing empirical hypotheses through controlled manipulation of material conditions and observation of outcomes." ;
    bfo:0000057 :Scientist ;
    cco:has_input :EmpiricalHypothesis, :ExperimentalConditions ;
    cco:has_output :VerificationResult ;
    :realizesValue :EmpiricalVerification, :EmpiricalTruth ;
    :methodologicalFeatures [
        :control "Control variables to isolate causal factor" ;
        :randomization "Random assignment to eliminate confounds" ;
        :blinding "Blind observers to prevent expectation effects" ;
        :replication "Independent replication to confirm results"
    ] ;
    :yields "Empirically verified or falsified claims about material causation" ;
    :empiricalSupport "Randomized controlled trial (RCT) as gold standard in medicine, psychology" ;
    rdfs:comment "Experimental method is materialist epistemology par excellence: Manipulate material conditions, observe material effects. Evidence-based practice relies on this." .

